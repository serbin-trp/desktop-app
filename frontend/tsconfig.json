{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "paths": {
      "@storage": ["./src/core/storage/index.ts"],
      "@core/*": ["./src/core/*"],
      "@utils/*": ["./src/utils/*"],
      "@wails/*": ["./wailsjs/go/*"],
      "@desktop": ["./src/core/desktop/service.ts"],
      "@ui/*": ["./src/core/ui/*"],
      "@modules/*": ["./src/modules/*"],
      "@layout/*": ["./src/core/layout/*"],
      "@spartan-ng/ui-accordion-helm": [
